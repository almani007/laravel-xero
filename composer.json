{
  "name": "almani/laravel-xero",
  "description": "Laravel package to integrate Xero accounting API",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "illuminate/support": "^8.0|^9.0|^10.0",
    "guzzlehttp/guzzle": "^6.3|^7.0",
    "calcinai/xero-php": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "Almani\\Xero\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": ["Almani\\Xero\\XeroServiceProvider"],
      "aliases": {
        "Xero": "Almani\\Xero\\Facades\\Xero"
      }
    }
  },
  "minimum-stability": "stable"
}
